{
  "name": "@hypervideo-dev/cli",
  "version": "0.1.5",
  "type": "module",
  "description": "CLI for Hypervideo media transformations",
  "bin": {
    "hypervideo": "./dist/index.js"
  },
  "main": "./dist/index.js",
