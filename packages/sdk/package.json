{
  "name": "@hypervideo-dev/sdk",
  "version": "0.1.2",
  "description": "Official SDK for Hypervideo media transformation API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "hypervideo",
    "image-processing",
    "video-processing",
    "background-removal",
    "resize",
    "thumbnail",
    "sdk",
    "api-client"
  ],
  "author": "Hypersocial",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hypersocialinc/hypervideo.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://hypervideo.dev",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
